	chunk ::= {stat [`;´]}

	block ::= chunk

	stat ::=  var `=´ exp 
            | local Name [`=´ exp] 

	var ::=  Name

	exp ::=  nil 
           | false 
           | true 
           | Number 
           | prefixexp 
           | exp binop exp 
           | unop exp 

	prefixexp ::= var 
                | `(´ exp `)´

	binop ::= `*´ 
             | `/´ 
             | `-´ 
             | `+`

	unop ::= `-´ 
            | not 
            | `#´
